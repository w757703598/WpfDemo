<Window x:Class="资源字典.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:资源字典"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525" Loaded="Window_Loaded">

    <StackPanel Margin="5">
        <Button x:Name="button" Content="Button" Margin="5" Padding="5" FontSize="14" Background="{DynamicResource TitleBrush1}"/>
        <Button x:Name="button1" Content="Button1" Margin="5" Padding="5" FontSize="14" Background="{DynamicResource TitleBrush2}"/>
        <Button x:Name="button2" Content="Button2" Margin="5" Padding="5" FontSize="14" Click="button2_Click" />
    </StackPanel>
</Window>
